{
  "name": "vite-nuxt",
  "type": "module",
  "version": "1.25.1029",
  "private": true,
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "build": "nuxi build --analyze",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "serve": "nuxi dev --public",
    "generate": "nuxi generate",
    "preview": "NITRO_PORT=5123 node .output/server/index.mjs",
    "start": "nuxi build && NITRO_PORT=5123 node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc -b --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "postinstall": "nuxi prepare",
    "prepare[-del]": "npx simple-git-hooks",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@lincy/async-validation": "catalog:prod",
    "@lincy/utils": "catalog:prod",
    "@nuxt/image": "catalog:nuxt",
    "@tato30/vue-pdf": "catalog:prod",
    "better-sqlite3": "catalog:prod",
    "element-plus": "catalog:prod",
    "markdown-it-link-attributes": "catalog:prod",
    "md-editor-v3": "catalog:prod",
    "md5": "catalog:prod",
    "pinia": "catalog:prod",
    "store2": "catalog:prod"
  },
  "devDependencies": {
    "@element-plus/nuxt": "catalog:nuxt",
    "@iconify-json/carbon": "catalog:icons",
    "@iconify-json/twemoji": "catalog:icons",
    "@lincy/eslint-config": "catalog:lint",
    "@lincy/stylelint-config": "catalog:lint",
    "@lincy/unocss-base-config": "catalog:dev",
    "@nuxt/kit": "catalog:nuxt",
    "@nuxtjs/color-mode": "catalog:nuxt",
    "@pinia/nuxt": "catalog:nuxt",
    "@types/markdown-it-link-attributes": "catalog:types",
    "@types/md5": "catalog:types",
    "@types/mockjs": "catalog:types",
    "@unocss/eslint-config": "catalog:lint",
    "@unocss/nuxt": "catalog:nuxt",
    "@vite-pwa/nuxt": "catalog:nuxt",
    "@vue-macros/nuxt": "catalog:nuxt",
    "@vueuse/nuxt": "catalog:nuxt",
    "consola": "catalog:dev",
    "eslint": "catalog:lint",
    "eslint-plugin-format": "catalog:lint",
    "lint-staged": "catalog:lint",
    "mockjs": "catalog:dev",
    "nuxt": "catalog:nuxt",
    "postcss-px-to-viewport-8-plugin": "catalog:dev",
    "sass": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "stylelint": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vue-tsc": "catalog:dev"
  },
  "simple-git-hooks[-del]": {
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged[-del]": {
    "*.{[jt]s?(x),vue,md}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
