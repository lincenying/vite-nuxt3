    "@nuxtjs/color-mode": "^3.3.2",
    "@pinia/nuxt": "^0.5.1",
    "@types/md5": "^2.3.5",
    "@unocss/eslint-config": "^0.58.4",
    "@unocss/nuxt": "^0.58.4",
    "@vite-pwa/nuxt": "^0.4.0",
